<?xml version="1.0"?>
<rdf:RDF xmlns="https://raw.githubusercontent.com/DevHassanMehdi/semi-final-assignment/refs/heads/main/ontology.owl#"
     xml:base="https://raw.githubusercontent.com/DevHassanMehdi/semi-final-assignment/refs/heads/main/ontology.owl"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

<owl:Ontology rdf:about="https://raw.githubusercontent.com/DevHassanMehdi/semi-final-assignment/refs/heads/main/ontology.owl"/>

<!-- Classes -->
<owl:Class rdf:about="#Person"/>
<owl:Class rdf:about="#Student">
    <rdfs:subClassOf rdf:resource="#Person"/>
</owl:Class>
<owl:Class rdf:about="#UndergraduateStudent">
    <rdfs:subClassOf rdf:resource="#Student"/>
</owl:Class>
<owl:Class rdf:about="#GraduateStudent">
    <rdfs:subClassOf rdf:resource="#Student"/>
</owl:Class>
<owl:Class rdf:about="#Instructor">
    <rdfs:subClassOf rdf:resource="#Person"/>
</owl:Class>
<owl:Class rdf:about="#TeachingAssistant">
    <rdfs:subClassOf rdf:resource="#Person"/>
</owl:Class>

<owl:Class rdf:about="#TeachingStaff">
    <owl:equivalentClass>
        <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
                <rdf:Description rdf:about="#Instructor"/>
                <rdf:Description rdf:about="#TeachingAssistant"/>
            </owl:unionOf>
        </owl:Class>
    </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#NonInstructor">
    <owl:equivalentClass>
        <owl:Class>
            <owl:intersectionOf rdf:parseType="Collection">
                <rdf:Description rdf:about="#Person"/>
                <owl:Class>
                    <owl:complementOf rdf:resource="#Instructor"/>
                </owl:Class>
            </owl:intersectionOf>
        </owl:Class>
    </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#Department"/>
<owl:Class rdf:about="#Program"/>
<owl:Class rdf:about="#DegreeProgram">
    <rdfs:subClassOf rdf:resource="#Program"/>
</owl:Class>
<owl:Class rdf:about="#MinorProgram">
    <rdfs:subClassOf rdf:resource="#Program"/>
</owl:Class>
<owl:Class rdf:about="#Course"/>
<owl:Class rdf:about="#LectureCourse">
    <rdfs:subClassOf rdf:resource="#Course"/>
</owl:Class>
<owl:Class rdf:about="#LabCourse">
    <rdfs:subClassOf rdf:resource="#Course"/>
</owl:Class>
<owl:Class rdf:about="#SeminarCourse">
    <rdfs:subClassOf rdf:resource="#Course"/>
</owl:Class>

<owl:Class rdf:about="#LabOrLectureCourse">
    <owl:equivalentClass>
        <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
                <rdf:Description rdf:about="#LabCourse"/>
                <rdf:Description rdf:about="#LectureCourse"/>
            </owl:unionOf>
        </owl:Class>
    </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#Assessment"/>
<owl:Class rdf:about="#Exam">
    <rdfs:subClassOf rdf:resource="#Assessment"/>
</owl:Class>
<owl:Class rdf:about="#Assignment">
    <rdfs:subClassOf rdf:resource="#Assessment"/>
</owl:Class>
<owl:Class rdf:about="#Room"/>
<owl:Class rdf:about="#Building"/>
<owl:Class rdf:about="#Enrollment"/>
<owl:Class rdf:about="#Semester"/>

<owl:Class rdf:about="#Grade">
    <owl:oneOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#gradeA"/>
        <rdf:Description rdf:about="#gradeB"/>
        <rdf:Description rdf:about="#gradeC"/>
        <rdf:Description rdf:about="#gradeD"/>
        <rdf:Description rdf:about="#gradeE"/>
        <rdf:Description rdf:about="#gradeF"/>
        <rdf:Description rdf:about="#gradePass"/>
        <rdf:Description rdf:about="#gradeFail"/>
    </owl:oneOf>
</owl:Class>

<owl:Class rdf:about="#DepartmentalCourse">
    <owl:equivalentClass>
        <owl:Class>
            <owl:intersectionOf rdf:parseType="Collection">
                <rdf:Description rdf:about="#Course"/>
                <owl:Restriction>
                    <owl:onProperty rdf:resource="#offeredBy"/>
                    <owl:allValuesFrom rdf:resource="#Department"/>
                </owl:Restriction>
            </owl:intersectionOf>
        </owl:Class>
    </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#EnrolledStudent">
    <owl:equivalentClass>
        <owl:Class>
            <owl:intersectionOf rdf:parseType="Collection">
                <rdf:Description rdf:about="#Student"/>
                <owl:Restriction>
                    <owl:onProperty rdf:resource="#enrolledIn"/>
                    <owl:someValuesFrom rdf:resource="#Course"/>
                </owl:Restriction>
            </owl:intersectionOf>
        </owl:Class>
    </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#AdvisedStudent">
    <owl:equivalentClass>
        <owl:Class>
            <owl:intersectionOf rdf:parseType="Collection">
                <rdf:Description rdf:about="#Student"/>
                <owl:Restriction>
                    <owl:onProperty rdf:resource="#hasAdvisor"/>
                    <owl:onClass rdf:resource="#Instructor"/>
                    <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                </owl:Restriction>
            </owl:intersectionOf>
        </owl:Class>
    </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#SingleRoomCourse">
    <owl:equivalentClass>
        <owl:Class>
            <owl:intersectionOf rdf:parseType="Collection">
                <rdf:Description rdf:about="#Course"/>
                <owl:Restriction>
                    <owl:onProperty rdf:resource="#heldIn"/>
                    <owl:allValuesFrom rdf:resource="#Room"/>
                </owl:Restriction>
                <owl:Restriction>
                    <owl:onProperty rdf:resource="#heldIn"/>
                    <owl:onClass rdf:resource="#Room"/>
                    <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                </owl:Restriction>
            </owl:intersectionOf>
        </owl:Class>
    </owl:equivalentClass>
</owl:Class>

<!-- Object Properties -->
<owl:ObjectProperty rdf:about="#enrolledIn">
    <rdfs:domain rdf:resource="#Student"/>
    <rdfs:range rdf:resource="#Course"/>
</owl:ObjectProperty>
<owl:ObjectProperty rdf:about="#teaches">
    <rdfs:domain rdf:resource="#Instructor"/>
    <rdfs:range rdf:resource="#Course"/>
</owl:ObjectProperty>
<owl:ObjectProperty rdf:about="#offeredBy">
    <rdfs:domain rdf:resource="#Course"/>
    <rdfs:range rdf:resource="#Department"/>
</owl:ObjectProperty>
<owl:ObjectProperty rdf:about="#hasPrerequisite">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    <rdfs:domain rdf:resource="#Course"/>
    <rdfs:range rdf:resource="#Course"/>
</owl:ObjectProperty>
<owl:ObjectProperty rdf:about="#hasTA">
    <rdfs:domain rdf:resource="#Course"/>
    <rdfs:range rdf:resource="#TeachingAssistant"/>
</owl:ObjectProperty>
<owl:ObjectProperty rdf:about="#hasAdvisor">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:domain rdf:resource="#Student"/>
    <rdfs:range rdf:resource="#Instructor"/>
</owl:ObjectProperty>
<owl:ObjectProperty rdf:about="#hasIDCard">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
    <rdfs:domain rdf:resource="#Person"/>
    <rdfs:range rdf:resource="#IDCard"/>
</owl:ObjectProperty>
<owl:ObjectProperty rdf:about="#assessedBy">
    <rdfs:domain rdf:resource="#Course"/>
    <rdfs:range rdf:resource="#Assessment"/>
</owl:ObjectProperty>
<owl:ObjectProperty rdf:about="#heldIn">
    <rdfs:domain rdf:resource="#Course"/>
    <rdfs:range rdf:resource="#Room"/>
</owl:ObjectProperty>
<owl:ObjectProperty rdf:about="#hasMember">
    <rdfs:domain rdf:resource="#Department"/>
    <rdfs:range rdf:resource="#Person"/>
</owl:ObjectProperty>
<owl:ObjectProperty rdf:about="#identifies">
    <owl:inverseOf rdf:resource="#hasIDCard"/>
    <rdfs:domain rdf:resource="#IDCard"/>
    <rdfs:range rdf:resource="#Person"/>
</owl:ObjectProperty>

<!-- Datatype Properties -->
<owl:DatatypeProperty rdf:about="#hasName">
    <rdfs:domain rdf:resource="#Person"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>
<owl:DatatypeProperty rdf:about="#hasEmail">
    <rdfs:domain rdf:resource="#Person"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>
<owl:DatatypeProperty rdf:about="#hasUniversityId">
    <rdfs:domain rdf:resource="#Person"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>
<owl:DatatypeProperty rdf:about="#hasCredits">
    <rdfs:domain rdf:resource="#Course"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
</owl:DatatypeProperty>
<owl:DatatypeProperty rdf:about="#hasECTS">
    <rdfs:domain rdf:resource="#Course"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
</owl:DatatypeProperty>
<owl:DatatypeProperty rdf:about="#hasStartDate">
    <rdfs:domain rdf:resource="#Course"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
</owl:DatatypeProperty>
<owl:DatatypeProperty rdf:about="#hasEndDate">
    <rdfs:domain rdf:resource="#Course"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
</owl:DatatypeProperty>
<owl:DatatypeProperty rdf:about="#hasEnrollmentYear">
    <rdfs:domain rdf:resource="#Student"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#gYear"/>
</owl:DatatypeProperty>
<owl:DatatypeProperty rdf:about="#hasGPA">
    <rdfs:domain rdf:resource="#Student"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
</owl:DatatypeProperty>
<owl:DatatypeProperty rdf:about="#hasCapacity">
    <rdfs:domain rdf:resource="#Room"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
</owl:DatatypeProperty>

<!-- Other Classes -->
<owl:Class rdf:about="#IDCard"/>

<!-- Grade individuals -->
<rdf:Description rdf:about="#gradeA"><rdf:type rdf:resource="#Grade"/></rdf:Description>
<rdf:Description rdf:about="#gradeB"><rdf:type rdf:resource="#Grade"/></rdf:Description>
<rdf:Description rdf:about="#gradeC"><rdf:type rdf:resource="#Grade"/></rdf:Description>
<rdf:Description rdf:about="#gradeD"><rdf:type rdf:resource="#Grade"/></rdf:Description>
<rdf:Description rdf:about="#gradeE"><rdf:type rdf:resource="#Grade"/></rdf:Description>
<rdf:Description rdf:about="#gradeF"><rdf:type rdf:resource="#Grade"/></rdf:Description>
<rdf:Description rdf:about="#gradePass"><rdf:type rdf:resource="#Grade"/></rdf:Description>
<rdf:Description rdf:about="#gradeFail"><rdf:type rdf:resource="#Grade"/></rdf:Description>

</rdf:RDF>
